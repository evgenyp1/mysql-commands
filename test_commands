CREATE TABLE account_compliance_review_events
(
    instance_id                       BINARY(16)      NOT NULL,
    account_compliance_review_id      BINARY(16)      NOT NULL,
    account_compliance_review_version INT UNSIGNED    NOT NULL,
    event_time                        BIGINT UNSIGNED NOT NULL,
    event_name                        VARCHAR(50)     NOT NULL,
    event_data                        BLOB,
    event_metadata                    BLOB,
    PRIMARY KEY (instance_id, account_compliance_review_id, account_compliance_review_version)
)
    ENGINE = InnoDB
    DEFAULT CHARSET = UTF8;
